target 'LoginTest' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Presentation
  pod 'IQKeyboardManagerSwift'
  
  # Infrastructure
  pod 'FirebaseUI'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Phone'
  pod 'Firebase/Core'
  pod 'Firebase/Database'

end
